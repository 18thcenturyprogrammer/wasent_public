<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="frg"
            type="com.centuryprogrammer18thwasentsingleland.drop_pickup.UserHistoryMakeInvoiceFrg" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".drop_pickup.UserHistoryMakeInvoiceFrg">


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvInvoiceUserHistoryMakeInvoiceFrg"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="2dp"
            android:layout_weight="6">

        </androidx.recyclerview.widget.RecyclerView>

        <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:onClick="@{()-> frg.onClickUserInfosBtn()}"
            android:text="@string/user_infos" />
    </LinearLayout>
</layout>
