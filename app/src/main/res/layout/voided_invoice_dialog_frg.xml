<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <data>
        <variable
            name="frg"
            type="com.centuryprogrammer18thwasentsingleland.invoice_works.VoidInvoiceDialogFrg" />
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/btnCancelVoidedInvoiceDialogFrg"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:layout_weight="1"
            android:text="@string/cancel"
            android:onClick="@{()-> frg.onClickCancelBtn()}"/>

        <Button
            android:id="@+id/btnRemoveVoidedInvoiceDialogFrg"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="16dp"
            android:layout_weight="1"
            android:text="@string/remove"
            android:onClick="@{()-> frg.onClickRemvoBtn()}"/>
    </LinearLayout>
</layout>
